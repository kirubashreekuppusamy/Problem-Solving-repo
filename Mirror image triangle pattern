n=4

for i in range(1,n+1):
    k=0
    for j in range(1,n+1):
        k+=1
        if i<=j:
            print(k,end=" ")
        elif i>j:
            print("",end=" ")
    print(" ")
   
for i in range(n-1,0,-1):
    k=0
    for j in range(1,n+1):
        k+=1
        if i<=j:
            print(k,end=" ")
        elif i>j:
            print("",end=" ")
            
    print(" ")
    