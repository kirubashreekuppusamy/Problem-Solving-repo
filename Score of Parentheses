class Solution:
    def scoreOfParentheses(self, s: str) -> int:
        c=['(' ]
        stack=[]
        count=0
        for ch in s:
            if ch in c:
                stack.append(ch)
            top=stack[-1]
            if (len(stack)!=0 and (top=='(' and ch==')')):
                count+=1
                stack.pop()
        return count
        